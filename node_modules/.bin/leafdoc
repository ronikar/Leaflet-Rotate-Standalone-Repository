#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/js" ]; then
  "$basedir/js"  "$basedir/../leafdoc/src/cli.js" "$@"
  ret=$?
else 
  js  "$basedir/../leafdoc/src/cli.js" "$@"
  ret=$?
fi
exit $ret
